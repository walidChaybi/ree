  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "NODE_ENV=test vitest run --config vite.config.mts --coverage.enabled --coverage.reporter lcov --silent --bail 1",
    "test:dev": "NODE_ENV=test vitest --config vite.config.mts",
    "test:dev:coverage": "NODE_ENV=test vitest --config vite.config.mts --coverage",
    "package": "npm run build",
    "transpil": "tsc",
    "unused": "ts-unused-exports tsconfig.json",
    "cy:open:dev": "npx cypress open --config-file e2e/cypress.config.ts",
    "cy:run:dev": "npx cypress run --config-file e2e/cypress.config.ts --browser firefox",
    "cy:open:uat1": "npx cypress open --config-file e2e/uat1.config.ts",
    "cy:run:uat1": "npx cypress run --config-file e2e/uat1.config.ts --browser firefox"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "e2e/support/step_definitions/**/*.{js,ts}"
  }
