  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test:devw": "node scripts/test.js --watchAll",
    "test:dev": "CI=true node scripts/test.js",
    "test:dev:update": "node scripts/test.js --watchAll -u",
    "test:dev:coverage": "node scripts/test.js --coverage --watchAll=false",
    "test": "CI=true node scripts/test.js --coverage",
    "package": "node scripts/build.js",
    "publish:pact": "node pact/publish/publish.js"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter",
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "!<rootDir>/src/serviceWorker.ts",
      "**/*.{ts,tsx}",
      "!**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "@composant/(.+)$": "<rootDir>/src/views/common/composant/$1",
      "@hook/(.+)$": "<rootDir>/src/views/common/hook/$1",
      "@util/(.+)$": "<rootDir>/src/views/common/util/$1",
      "@utilMetier/(.+)$": "<rootDir>/src/views/common/utilMetier/$1",
      "@widget/(.+)$": "<rootDir>/src/views/common/widget/$1",
      "@core/(.+)$": "<rootDir>/src/views/core/$1",
      "@pages/(.+)$": "<rootDir>/src/views/pages/$1",
      "@router/(.+)$": "<rootDir>/src/views/router/$1",
      "@api/(.+)$": "<rootDir>/src/api/$1",
      "@model/(.+)$": "<rootDir>/src/model/$1",
      "@scss/(.+)$": "<rootDir>/src/scss/$1",
      "@mock/(.+)$": "<rootDir>/src/mock/$1"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "jestSonar": {
    "reportPath": "test_report/jest",
    "reportFile": "test-report.xml"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      ">0.2%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
